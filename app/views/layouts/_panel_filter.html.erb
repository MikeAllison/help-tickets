<div class="row">
	<div class="col-xs-12">
		<div class="panel panel-default">
		  <div class="panel-heading">
		    <h3><%= panel_header %></h3>
		  </div>
		  <div class="panel-body">
		    <form>
					<label for="filterText">Filter:&nbsp;</label>
					<input type="text" id="filterText">
					<button id="filterClear" class="btn btn-primary">Clear</button>
					<%= link_to 'Cancel', :back, class: 'btn btn-default' %>
				</form>
		  </div>
		  <%= render 'list' %>
		</div>		
	</div>
</div>