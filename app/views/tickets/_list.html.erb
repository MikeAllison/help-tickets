<table class="table table-striped">
	<thead>
		<th>Ticket &#35;</th>
		<th>Requested For</th>
		<th>Topic</th>
		<th>Description</th>
		<th>Status</th>
		<th>Created</th>
		<th>Updated</th>
	</thead>
	<tbody>
		<% @tickets.each do |ticket| %>
			<tr>
				<td><%= ticket.id %></td>
				<td><%= ticket.employee.last_first %></td>
				<td><%= ticket.topic.system %></td>
				<td><%= ticket.description %></td>
				<td><%= ticket.status.state %></td>
				<td><%= ticket.created_at.strftime('%m/%d/%Y %I:%M %p') %></td>
				<td><%= ticket.updated_at.strftime('%m/%d/%Y %I:%M %p') %></td>
			</tr>
		<% end %>
</table>