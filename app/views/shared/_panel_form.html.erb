<div class="row">
	<div class="col-xs-12">
		<div class="panel panel-default">
		  <div class="panel-heading">
		    <h3><%= page_header %></h3>
		  </div>
		  <div class="panel-body">
		  	<% if controller_name == 'tickets' %>
		  		<% if admin? %>
		  			<%= render 'tickets/form_admin' %>
		  		<% else %>
		  			<%= render 'tickets/form' %>
		  		<% end %>
		  	<% else %>
		  		<%= render 'form' %>
		  	<% end %>
		  </div>
		</div>
	</div>
</div>
